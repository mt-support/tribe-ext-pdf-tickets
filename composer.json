{
  "name": "tribe-ext-pdf-tickets",
  "description": "Tickets (RSVP, Tribe Commerce PayPal, WooCommerce, and Easy Digital Downloads) become PDFs saved to your Uploads directory and attached to the ticket email.",
  "type": "project",
  "require": {
    "mpdf/mpdf": "^7.0.3"
  },
  "license": "GPL-2.0-only",
  "authors": [
    {
      "name": "Modern Tribe, Inc.",
      "email": "support@theeventscalendar.com"
    }
  ],
  "minimum-stability": "stable",
  "archive": {
    "exclude": [
      "*.zip",
      ".gitignore",
      ".github",
      ".travis.yml",
      "build-phar.sh",
      "composer.json",
      "composer.lock",
      "phpunit.xml",
      "psalm.xml",
      "README.md",
      "ruleset.xml",
      "vendor/mpdf/**/ttfonts"
    ]
  },
  "optimize-autoloader": true
}